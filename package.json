{
  "name": "gitmoji-cli",
  "version": "1.1.0",
  "description": "A gitmoji client for using emojis on commit messages.",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "gitmoji": "cli.js"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "array-callback-return": 1
  },
  "ignores": [
	"test/**"
  ]
  },
  "scripts": {
    "lint": "xo",
	"mocha": "mocha ./test/*.js",
    "test": "npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carloscuesta/gitmoji-cli.git"
  },
  "keywords": [
    "gitmoji",
    "emoji",
    "carloscuesta",
    "commit"
  ],
  "author": "Carlos Cuesta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carloscuesta/gitmoji-cli/issues"
  },
  "homepage": "https://github.com/carloscuesta/gitmoji-cli#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "chalk": "^1.1.3",
    "execa": "^0.5.0",
    "inquirer": "^1.2.3",
    "meow": "^3.7.0",
    "path-exists": "^3.0.0",
    "update-notifier": "^1.0.2"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "should": "^11.1.1",
    "xo": "^0.17.1"
  }
}
